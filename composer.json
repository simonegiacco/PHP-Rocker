    "minimum-stability": "dev",
    "homepage" : "https://github.com/victorjonsson/PHP-Rocker#php-rocker",
    "keywords" : ["rest", "restful", "webservice", "API", "http"],
    "license" : "MIT",
    "version" : "1.3.0",
    "authors": [{
        "name": "Victor Jonsson",
        "email": "kontakt@victorjonsson.se"
    }],
    "require" : {
        "php" : ">=5.3.2",
        "fridge/dbal": "dev-master",
        "slim/slim": "2.*",
        "wp-cli/php-cli-tools": "0.10.5",
        "guzzle/http": "3.3.1.0",
        "gregwar/image": "dev-master"
    },
    "require-dev" : {
        "phpbenchmark/phpbenchmark": "dev-master",
        "phpunit/phpunit": ">=3.7.1"
    },
    "autoload": {
        "psr-0": {"Rocker": "libs/"}
    }
}
